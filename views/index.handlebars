  <div class="nav-wrapper">
    <div id="siteName"><a href="#" class="brand-logo">       Irish Music Magazine Scraper</a></div>
    <ul id="nav-mobile" class="right hide-on-med-and-down">
      <li><a href="#">Home</a></li>
      <li><a href="/saved"><i class="material-icons left">save</i>Saved Articles</a></li>
      <li>
        <a class="waves-effect waves-light btn"><i class="material-icons left">local_library</i>Scrape New Articles</a>
      </li>
      <li>
        <a class="waves-effect waves-light btn"><i class="material-icons left">refresh</i>Clear Articles</a>
      </li>
    </ul>
  </div>
</nav>
<div class="parallax-container">
  <div class="parallax"><img src="images/anna-church-VM3u6iDvWDg-unsplash.jpg"></div>
  <br>
  <div class="container">
    <div id="stories">
        {{#each story}}
    <div class="section white">
      <div class="row container">
        <h2 class="header data-title" id="title">{{title}}</h2>
        <p id="content" class="grey-text text-darken-3 lighten-3 data-content">{{content}}</p>
        <a class="data-link" id = "link" href={{link}}>Read More</a>
        <a class="waves-effect waves-light btn saveThis"><i class="material-icons left">save</i>Save Article</a>
      </div>
    </div>
    <br>
      {{/each}}
      </div>
  </div>
</div>